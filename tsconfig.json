{
    "compileOnSave": true,
    "compilerOptions": {
      "target": "ES5",
      "downlevelIteration": true,
      "module": "commonjs",
      "declaration": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "removeComments": true,
      "baseUrl": ".",
      "paths": {
        "mathjax-full": ["node_modules/mathjax-full"]
      },
      "lib": ["es6", "dom"],
      "noLib": false,
      "sourceMap": true,
      "outDir": "js",
      "typeRoots": ["./typings"]
    },
    "include": ["ts/**/*"],
    "exclude": ["js", "es5", "components"]
  }
